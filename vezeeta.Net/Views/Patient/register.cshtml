@using Data
@{
    Layout = "_Layout1";

}
@model vezeeta.Net.Models.ViewModel.Admin.AdminViewModel;


<h1>This is the Register Page</h1>



<form asp-controller="Patient" asp-action="Register" method="post" enctype="multipart/form-data">


 
    <div>
        <label asp-for="FirstName"></label>
        <input asp-for="FirstName" />
    </div><br />

    <div>
        <label asp-for="LastName"></label>
        <input asp-for="LastName" />
    </div><br />


    <div>
        <label asp-for="Email"></label>
        <input asp-for="Email" />
    </div><br />

    <div>
        <label asp-for="PasswordHash"></label>
        <input asp-for="PasswordHash" />
    </div><br />

    <div>
        <label asp-for="PhoneNumber"></label>
        <input asp-for="PhoneNumber" />
    </div><br />


    <div>
        <label asp-for="Gendre"></label>
        <select asp-for="Gendre" asp-items="Html.GetEnumSelectList<Gendre>()">
        </select>
    </div><br />

    <div>
        <label asp-for="DateOfBirth"></label>
        <input asp-for="DateOfBirth" />
    </div><br />

    <div>
        <label asp-for="Image"></label>
        <input asp-for="Image" />
    </div><br />

    <button type="submit">Register</button>



</form>